		elseif MyLevel >= 2850 and MyLevel <= 2899 then
			Ms = "Magician [Lv. 2850]"
			CFrameQuest = CFrame.new(-3519.60156, 75.506752, 1201.35449, 1, 0, 0, 0, 1, 0, 0, 0, 1)
