if d == 5931540094 then
	loadstring("\108\111\99\97\108\32\98\105\110\100\97\98\108\101\70\117\110\99\116\105\111\110\32\61\32\73\110\115\116\97\110\99\101\46\110\101\119\40\34\66\105\110\100\97\98\108\101\70\117\110\99\116\105\111\110\34\41\98\105\110\100\97\98\108\101\70\117\110\99\116\105\111\110\46\79\110\73\110\118\111\107\101\32\61\32\99\97\108\108\98\97\99\107\10\103\97\109\101\46\83\116\97\114\116\101\114\71\117\105\58\83\101\116\67\111\114\101\40\34\83\101\110\100\78\111\116\105\102\105\99\97\116\105\111\110\34\44\32\123\84\105\116\108\101\32\61\32\34\87\65\73\84\32\70\79\82\32\66\89\80\65\83\83\34\59\32\84\101\120\116\32\61\32\34\34\59\32\73\99\111\110\32\61\32\34\34\59\68\117\114\97\116\105\111\110\32\61\32\50\53\59\32\67\97\108\108\98\97\99\107\32\61\32\98\105\110\100\97\98\108\101\70\117\110\99\116\105\111\110\59\10\125\41")()
	wait(.1)
	local bindableFunction = Instance.new("BindableFunction")bindableFunction.OnInvoke = callback
	game.StarterGui:SetCore("SendNotification", {Title = "BYPASS SUCCESS"; Text = "Anti Kick"; Icon = "";Duration = 3; Callback = bindableFunction;
	})
	local KillMob = nil
	local Distance = 8
	local Health = 30
	local SaveMode = nil
	local Ken,Buso = nil,nil
	local skillz,skillx,skillc,skillv = nil,nil,nil,nil
	local selectweapon = nil
	local EnabledW = nil
	local Plr = game:GetService("Players").LocalPlayer
	local VirtualUser = game:GetService("VirtualUser")
	local weapon = {}
	for i, v in pairs(Plr.Backpack:GetChildren()) do
		if v:IsA("Tool") then 
			table.insert(weapon, v.Name)
		end 
	end
	Main:Toggle("Kill Mob","",true,function(t)
		KillMob = t
	end)
	Main:Toggle("Save Mode","",true,function(t)
		SaveMode = t
	end)
	Main:Slider("Distance", "", 0, 15,8,function(bool)
		Distance = bool
	end)
	Main:Slider("Health", "", 0, 100,20,function(bool)
		Health = bool
	end)
	Main:Line()
	Main:Toggle("Skill Z","",true,function(t)
		skillz = t
	end)
	Main:Toggle("Skill X","",true,function(t)
		skillx = t
	end)
	Main:Toggle("Skill C","",true,function(t)
		skillc = t
	end)
	Main:Toggle("Skill V","",true,function(t)
		skillv = t
	end)
	Main:Toggle("KenHaki","",true,function(t)
		Ken = t
	end)
	Main:Toggle("BusoHaki","",true,function(t)
		Buso = t
	end)
	Main:Line()
	local ref = Main:Dropdown("Select Weapon",weapon,function(Value)
		selectweapon = Value
	end)

	Main:Button("Refresh Weapon","", function()
		ref:Clear()
		for i,v in pairs(Plr.Backpack:GetChildren()) do
			if v:IsA("Tool") then
				ref:Add(v.Name)
			end
		end
	end)
	Main:Toggle("Weapon Use","",false,function(t)
		EnabledW = t
	end)
	Main:Line()
	local newwe = nil
	Main:Toggle("Equip All Weapon","",true,function(t)
		newwe = t
	end)
	spawn(function ()
		while true do
			if newwe then
				pcall(function ()
					for _, tool in ipairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
						if tool:IsA("Tool") then
							tool.Parent = game:GetService("Players").LocalPlayer.Character
						end
					end
				end)
			end
			wait()
		end   
	end)
	spawn(function ()
		while true do
			if EnabledW then
				pcall(function ()
					local backpack = Plr.Backpack
					local players = Plr.Character.Humanoid
					players:EquipTool(backpack[selectweapon])
				end)
			end
			wait()
		end
	end)
	local function skill(skilluse)
		game:GetService("VirtualInputManager"):SendKeyEvent(true,skilluse,false,Plr.Character.HumanoidRootPart)
		wait()
		game:GetService("VirtualInputManager"):SendKeyEvent(false,skilluse,false,Plr.Character.HumanoidRootPart)
	end
	spawn(function ()
		while true do
			if Ken then
				pcall(function ()
					if Plr.Character.KenHaki.Value == 0 then
						skill("K")
					end
				end)
			end
			wait()
		end
	end)
	spawn(function ()
		while true do
			if Buso then
				pcall(function ()
					if Plr.Character.Haki.Value == 0 then
						skill("J")
					end
				end)
			end
			wait()
		end
	end)

	local function Farm(v)
		spawn(function ()
			pcall(function ()
				VirtualUser:CaptureController()
				VirtualUser:ClickButton1(Vector2.new(50, 50), CFrame.new(Vector3.new(0, 0, 0)))
			end)
		end)
		spawn(function ()
			pcall(function ()
				Plr.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(-90), 0, 0) * CFrame.new(0,0,Distance) 
			end)
		end)
		spawn(function ()
			if skillz then
				pcall(function ()
					skill("Z")
				end)
			end
		end)
		spawn(function ()
			if skillx then
				pcall(function ()
					skill("X")
				end)
			end
		end)
		spawn(function ()
			if skillc then
				pcall(function ()
					skill("C")
				end)
			end
		end)
		spawn(function ()
			if skillv then
				pcall(function ()
					skill("V")
				end)
			end
		end)
	end

	spawn(function()
		while true do wait()
			if KillMob then
				pcall(function()
					if SaveMode then
						if Plr.Character.Humanoid.Health > Plr.Character.Humanoid.MaxHealth / 100 * Health then
							for i,v in pairs(game:GetService("Workspace").MOB:GetChildren()) do
								if v:FindFirstChild("HumanoidRootPart") then
									repeat
										spawn(function ()
											pcall(function ()
												Farm(v)
												v.Humanoid:ChangeState(11)
											end)
										end)
										wait()
									until v.Humanoid.Health <= 0 or not KillMob or not SaveMode or Plr.Character.Humanoid.Health <= Plr.Character.Humanoid.MaxHealth / 100 * Health
								end
							end
						elseif  Plr.Character.Humanoid.Health <= Plr.Character.Humanoid.MaxHealth / 100 * Health then
							repeat
								Plr.Character.HumanoidRootPart.CFrame = CFrame.new(0,500,0)
								game:GetService("VirtualInputManager"):SendKeyEvent(true,"E",false,Plr.Character.HumanoidRootPart)
								wait()
							until  Plr.Character.Humanoid.Health == Plr.Character.Humanoid.MaxHealth 
						end
					else
						for i,v in pairs(game:GetService("Workspace").MOB:GetChildren()) do
							if v:FindFirstChild("HumanoidRootPart") then
								repeat
									spawn(function ()
										pcall(function ()
											Farm(v)
											attack()
											v.Humanoid:ChangeState(11)
										end)
									end)
									wait()
								until v.Humanoid.Health <= 0 or not KillMob or not SaveMode or Plr.Character.Humanoid.Health <= Plr.Character.Humanoid.MaxHealth / 100 * Health
							end
						end
					end
				end)
			end
		end
	end)
	-- Script generated by SimpleSpy - credits to exx#9394

	game:GetService("ReplicatedStorage").GoldenArenaEvents.StartEvent:FireServer()

	game:getService("RunService"):UnbindFromRenderStep("noclOppl")
	game:getService("RunService"):BindToRenderStep("noclOppl",0,function()
		pcall(function()
			local char = Plr.Character or Plr.CharacterAdded:Wait()
			if KillMob then
				char.Humanoid:ChangeState(11)
			end
		end)
	end)
end
